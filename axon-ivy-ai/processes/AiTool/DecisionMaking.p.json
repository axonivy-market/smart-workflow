{
  "$schema" : "https://json-schema.axonivy.com/process/13.1.2/process.json",
  "id" : "197F79FF241C25AA",
  "kind" : "CALLABLE_SUB",
  "config" : {
    "data" : "AiTool.DecisionMakingData"
  },
  "elements" : [ {
      "id" : "f0",
      "type" : "CallSubStart",
      "name" : "AiDecide(AiOption>,String,AiExample>,List<String>,AbstractAiServiceConnector)",
      "config" : {
        "signature" : "AiDecide",
        "input" : {
          "params" : [
            { "name" : "options", "type" : "List<com.axonivy.utils.ai.dto.ai.AiOption>", "desc" : "" },
            { "name" : "query", "type" : "String", "desc" : "" },
            { "name" : "examples", "type" : "List<com.axonivy.utils.ai.dto.ai.AiExample>", "desc" : "" },
            { "name" : "customInstructions", "type" : "List<String>", "desc" : "" },
            { "name" : "connector", "type" : "com.axonivy.utils.ai.connector.AbstractAiServiceConnector", "desc" : "" }
          ],
          "map" : {
            "out.connector" : "param.connector",
            "out.customInstructions" : "param.customInstructions",
            "out.examples" : "param.examples",
            "out.options" : "param.options",
            "out.query" : "param.query"
          }
        },
        "result" : {
          "params" : [
            { "name" : "result", "type" : "String", "desc" : "" }
          ],
          "map" : {
            "result.result" : "in.result"
          }
        }
      },
      "visual" : {
        "at" : { "x" : 232, "y" : 136 }
      },
      "connect" : [
        { "id" : "f1", "to" : "f2" }
      ]
    }, {
      "id" : "f2",
      "type" : "SubProcessCall",
      "name" : "Setup AI Service caller",
      "config" : {
        "processCall" : "AiTool/Utils:setupAiServiceCaller(com.axonivy.utils.ai.connector.AbstractAiServiceConnector)",
        "call" : {
          "map" : {
            "param.connector" : "in.connector"
          }
        },
        "output" : {
          "map" : {
            "out" : "in",
            "out.connector" : "result.connector"
          }
        }
      },
      "visual" : {
        "at" : { "x" : 520, "y" : 136 }
      },
      "connect" : [
        { "id" : "f3", "to" : "f4" }
      ]
    }, {
      "id" : "f4",
      "type" : "Script",
      "name" : "Decide",
      "config" : {
        "output" : {
          "code" : [
            "import com.axonivy.utils.ai.dto.ai.AiVariable;",
            "import com.axonivy.utils.ai.enums.AiVariableState;",
            "import com.axonivy.utils.ai.function.DecisionMaker;",
            "",
            "DecisionMaker decisionMaker = DecisionMaker.getBuilder()",
            "  .useService(in.connector)",
            "  .addOptions(in.options)",
            "  .addCustomInstructions(in.customInstructions)",
            "  .withQuery(in.query).build();",
            "",
            "in.variable = decisionMaker.execute() as AiVariable;",
            "",
            "if (in.variable.state != AiVariableState.SUCCESS) {",
            "  in.result = \"\";",
            "}",
            "",
            "in.result = in.variable.getSafeValue();"
          ]
        },
        "sudo" : true
      },
      "visual" : {
        "at" : { "x" : 736, "y" : 136 }
      },
      "connect" : [
        { "id" : "f5", "to" : "f6", "color" : "default" }
      ]
    }, {
      "id" : "f6",
      "type" : "CallSubEnd",
      "visual" : {
        "at" : { "x" : 880, "y" : 136 }
      }
    } ]
}