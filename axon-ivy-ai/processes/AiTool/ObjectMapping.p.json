{
  "$schema" : "https://json-schema.axonivy.com/process/13.1.2/process.json",
  "id" : "197F7B1E77E4A5DA",
  "kind" : "CALLABLE_SUB",
  "config" : {
    "data" : "AiTool.ObjectMappingData"
  },
  "elements" : [ {
      "id" : "f0",
      "type" : "CallSubStart",
      "name" : "AiObjectMapping",
      "config" : {
        "signature" : "AiObjectMapping",
        "input" : {
          "params" : [
            { "name" : "query", "type" : "String", "desc" : "" },
            { "name" : "targetObject", "type" : "Object", "desc" : "" },
            { "name" : "examples", "type" : "List<com.axonivy.utils.ai.dto.ai.AiExample>", "desc" : "" },
            { "name" : "customInstructions", "type" : "List<String>", "desc" : "" },
            { "name" : "connector", "type" : "com.axonivy.utils.ai.connector.AbstractAiServiceConnector", "desc" : "" },
            { "name" : "fieldExplanations", "type" : "List<com.axonivy.utils.ai.dto.ai.FieldExplanation>", "desc" : "" }
          ],
          "map" : {
            "out.connector" : "param.connector",
            "out.customInstructions" : "param.customInstructions",
            "out.exampleJson" : "param.examples",
            "out.fieldExplanations" : "param.fieldExplanations",
            "out.query" : "param.query",
            "out.targetObject" : "param.targetObject"
          }
        },
        "result" : {
          "params" : [
            { "name" : "result", "type" : "Object", "desc" : "" }
          ],
          "map" : {
            "result.result" : "in.resultObject"
          }
        }
      },
      "visual" : {
        "at" : { "x" : 320, "y" : 128 },
        "labelOffset" : { "x" : 17, "y" : -23 }
      },
      "connect" : [
        { "id" : "f1", "to" : "f4" }
      ]
    }, {
      "id" : "f4",
      "type" : "SubProcessCall",
      "name" : "Setup AI Service caller",
      "config" : {
        "processCall" : "AiTool/Utils:setupAiServiceCaller(com.axonivy.utils.ai.connector.AbstractAiServiceConnector)",
        "call" : {
          "map" : {
            "param.connector" : "in.connector"
          }
        },
        "output" : {
          "map" : {
            "out" : "in",
            "out.connector" : "result.connector"
          }
        }
      },
      "visual" : {
        "at" : { "x" : 480, "y" : 128 }
      },
      "connect" : [
        { "id" : "f5", "to" : "f8", "color" : "default" }
      ]
    }, {
      "id" : "f8",
      "type" : "Script",
      "name" : "Object mapping",
      "config" : {
        "output" : {
          "code" : [
            "import com.axonivy.utils.ai.persistence.converter.BusinessEntityConverter;",
            "import com.axonivy.utils.ai.dto.ai.AiVariable;",
            "import com.axonivy.utils.ai.function.DataMapping;",
            "",
            "DataMapping mapper = DataMapping.getBuilder().withQuery(in.query)",
            "  .useService(in.connector)",
            "  .withObject(in.targetObject)",
            "  .addExamples(in.exampleJson)",
            "  .addFieldExplanations(in.fieldExplanations)",
            "  .addCustomInstructions(in.customInstructions)",
            "  .withInstructionsStrategy().build();",
            "",
            "AiVariable result = mapper.execute();",
            "in.resultObject = BusinessEntityConverter.jsonValueToEntity(result.content, Object.class);"
          ]
        }
      },
      "visual" : {
        "at" : { "x" : 640, "y" : 128 }
      },
      "connect" : [
        { "id" : "f9", "to" : "f12", "color" : "default" }
      ]
    }, {
      "id" : "f12",
      "type" : "CallSubEnd",
      "visual" : {
        "at" : { "x" : 768, "y" : 128 }
      }
    } ]
}