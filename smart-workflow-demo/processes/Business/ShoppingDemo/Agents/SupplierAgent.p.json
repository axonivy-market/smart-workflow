        "input" : {
          "params" : [
            { "name" : "supplier", "type" : "com.axonivy.utils.smart.workflow.demo.shopping.supplier.Supplier", "desc" : "All information about supplier" },
            { "name" : "instructions", "type" : "String", "desc" : "" }
          ],
          "map" : {
            "out.instructions" : "param.instructions",
            "out.supplier" : "param.supplier"
          }
        },
        "result" : {
          "params" : [
            { "name" : "response", "type" : "com.axonivy.utils.smart.workflow.demo.shopping.supplier.SupplierAgentResponse", "desc" : "" }
          ],
          "map" : {
            "result.response" : "in.response"
          }
        }
      },
      "visual" : {
        "at" : { "x" : 96, "y" : 64 }
      },
      "connect" : [
        { "id" : "f2", "to" : "f3" }
      ]
    }, {
      "id" : "f1",
      "type" : "CallSubEnd",
      "visual" : {
        "at" : { "x" : 440, "y" : 64 }
      }
    }, {
      "id" : "f3",
      "type" : "ProgramInterface",
      "name" : "Supplier Agent",
      "config" : {
        "javaClass" : "com.axonivy.utils.smart.workflow.AgenticProcessCall",
        "userConfig" : {
          "model" : "\"gpt-4.1\"",
