          "resultType" : "com.axonivy.utils.smart.workflow.demo.shopping.supplier.SupplierAgentResponse.class",
          "tools" : "[\"findSupplier\",\"createSupplier\"]",
          "resultMapping" : "in.response",
          "query" : "This is supplier information\r\n---\r\n\r\n<%= dev.langchain4j.internal.Json.toJson(in.supplier) %>"
        }
      },
      "visual" : {
        "at" : { "x" : 272, "y" : 64 }
      },
      "connect" : [
        { "id" : "f4", "to" : "f1", "color" : "default" }
      ]
    } ]
