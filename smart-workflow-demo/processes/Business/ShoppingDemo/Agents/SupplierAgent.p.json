{
  "$schema" : "https://json-schema.axonivy.com/14.0-dev/project/process.json",
  "id" : "1994B9BEB8FDE469",
  "kind" : "CALLABLE_SUB",
  "config" : {
    "data" : "Business.ShoppingDemo.Agents.SupplierAgentData"
  },
  "elements" : [ {
      "id" : "f0",
      "type" : "CallSubStart",
      "name" : "callSupplierAgent(Supplier,String)",
      "config" : {
