{
  "$schema": "https://json-schema.axonivy.com/market/10.0.0/product.json",
  "minimumIvyVersion": "13.2.0",
  "installers": [
    {
      "id": "maven-import",
      "data": {
        "projects": [
          {
            "groupId": "com.axonivy.utils.ai",
            "artifactId": "smart-workflow-demo",
            "version": "${version}",
            "type": "iar"
          },
          {
            "groupId": "com.axonivy.utils.ai",
            "artifactId": "smart-workflow-openai",
            "version": "${version}",
            "type": "iar",
            "importInWorkspace": true
          },
          {
            "groupId": "com.axonivy.utils.ai",
            "artifactId": "smart-workflow-azure-openai",
            "version": "${version}",
            "type": "iar",
            "importInWorkspace": false
          },
                    {
            "groupId": "com.axonivy.utils.ai",
            "artifactId": "smart-workflow-gemini",
            "version": "${version}",
            "type": "iar",
            "importInWorkspace": false
          }
        ],
        "repositories": [
          {
            "id": "maven.axonivy.com",
            "url": "https://maven.axonivy.com",
            "snapshots": {
              "enabled": "true"
            }
          }
        ]
      }
    },
    {
      "id": "maven-dependency",
      "data": {
        "dependencies": [
          {
            "groupId": "com.axonivy.utils.ai",
            "artifactId": "smart-workflow",
            "version": "${version}",
            "type": "iar"
          }
        ],
        "repositories": [
          {
            "id": "maven.axonivy.com",
            "url": "https://maven.axonivy.com",
            "snapshots": {
              "enabled": "true"
            }
          }
        ]
      }
    }
  ]
}
